<div class="panel panel-flat">
    <div class="panel-heading">
        <h5 class="panel-title">Tabla de Clientes</h5>
        <div class="heading-elements">
            <ul class="icons-list">
                <li>
                    <a data-action="reload"></a>
                </li>
            </ul>
        </div>
    </div>

    <div class="panel-body">
    </div>

    <table class="table datatable-generated">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Apellidos</th>
                <th>Telefono</th>
                <th>Correo</th>
                <th>Sucursal</th>
                <th>Services</th>
                <th>Actions</th>
            </tr>
        </thead>
    </table>
</div>


<div id="editarCliente" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header bg-info">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h6 class="modal-title">Editar Cliente</h6>
                <input id="id" style="display:none">
            </div>

            <div class="modal-body">


                <form id="form-editar-cliente" class="form-validate-jquery">
                    <div class="form-group col-md-12">

                        <div class="col-md-4">
                            <label for="nombre">Nombre:</label>
                            <input id="nombreup" type="text" class="form-control maxlength-options" maxlength="20" minLength="3" placeholder="">
                        </div>

                        <div class="col-md-4">
                            <label for="apellidoP">Primer Apellido:</label>
                            <input id="apellidoPup" type="text" name="apellidoP" class="form-control maxlength-options" maxlength="20" minLength="2" placeholder="">
                        </div>

                        <div class="col-md-4">
                            <label for="apellidoM">Segundo Apellido:</label>
                            <input id="apellidoMup" type="text" name="apellidoM" class="form-control maxlength-options" maxlength="20" minLength="2" placeholder="">
                        </div>
                    </div>

                    <div class="form-group col-md-12">
                        <div class="col-md-4">
                            <label>Direccion: </label>
                            <input id="direccionup" type="text" name="direccion" class="form-control">

                        </div>
                        <div class="col-md-4">
                            <label for="email">Correo</label>
                            <input id="emailup" type="email" name="email" class="form-control" placeholder="">
                        </div>

                        <div class="col-md-4">
                            <label>Celular: </label>
                            <input id="telefonoup" type="text" class="form-control format-phone-number" placeholder="Ingresa tu numero Celular">
                        </div>
                    </div>
                </form>
            </div>

            <div id="error" class="alert alert-danger alert-styled-left alert-bordered" style="display:none">
                <span class="text-semibold">Oh pps!</span> Ocurrio un Error
                <h4><a id="textoError" href="#" class="alert-link"> Verifica los datos</a></h4>.
            </div>

            <div class="modal-footer">
                <!-- <button id="closes" type="button" class="btn btn-link" data-dismiss="modal">Close</button> -->
                <form-group>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    <button id="btn-Actualizar-cliente" type="button" class="btn btn-success " data-dismiss="modal">Guardar</button>
                </form-group>

            </div>
        </div>
    </div>
</div>
