<!-- Defailt scripts-->
<script src="https://code.jquery.com/jquery-1.10.0.min.js" charset="utf-8"></script>
<script type="text/javascript" src="/js/core/libraries/bootstrap.min.js"></script>
<script src="/js/Clientes/registrarCliente.js"></script>
<script type="text/javascript" src="/js/pages/form_validation.js"></script>
<script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
<script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/jquery-ui.min.js"></script>
<link rel="stylesheet" type="text/css" href="/styles/overhang.min.css" />
<script type="text/javascript" src="/js/overhang.min.js"></script>
<!-- 2 columns form -->
<form class="form-horizontal form-validate-jquery" action="#" id="registroCliente">
    <div class="panel panel-flat">
        <div class="panel-heading">
            <h5 class="panel-title">REGISTRO DE CLIENTES</h5>
            <div class="heading-elements">
                <ul class="icons-list">
                    <li>
                        <a data-action="collapse"></a>
                    </li>
                    <li>
                        <a data-action="reload"></a>
                    </li>
                    <li>
                        <a data-action="close"></a>
                    </li>
                </ul>
            </div>
        </div>

        <div class="panel-body">
            <div class="row">


                <legend class="text-semibold"><i class="icon-reading position-left"></i> DATOS PERSONALES</legend>
                <div class="form-group col-md-12">
                    <div class="form-group col-md-6">

                        <label>Nombre:</label>
                        <input id="nombre-cliente" type="text" class="form-control" placeholder="Nombre">

                    </div>

                    <div class="form-group col-md-3">
                        <label>Apelildo PATERNO:</label>
                        <input id="apaternoC" type="text" class="form-control" placeholder="Apellido Paterno">

                    </div>

                    <div class="form-group col-md-3">
                        <label>Apelildo Materno:</label>
                        <input id="amaternoC" type="text" class="form-control" placeholder="Apellido Materno">
                    </div>
                </div>
                  <div class="form-group col-md-12">
                <div class="form-group col-md-4">
                    <label>Direccion:</label>
                    <input id="direccion-Cliente" type="text" class="form-control" placeholder="Direccion">
                </div>
                <div class="form-group col-md-4">
                    <label>Telefono:</label>
                    <input id="telefono-Cliente" type="text" class="form-control format-phone-number" placeholder="054-131-3423">
                </div>


                <div class="form-group col-md-4">
                  <label class="control-label ">Email  <span class="text-danger">*</span></label>
                  <input type="email" name="email" class="form-control" id="email-Cliente" required="required" placeholder="Enter a valid email address">
                </div>

              </div>
                <!-- Email field -->


            </div>
            <div class="text-right form-group col-md-12">
                <button id="btn-registrarCLiente" type="submit" class="btn btn-primary">Registrar <i class="icon-arrow-right14 position-right"></i></button>
            </div>
        </div>
    </div>
</form>
<!-- /2 columns form -->
